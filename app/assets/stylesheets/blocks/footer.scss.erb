////////////////////////////////////////////////////////////////////*/

#footer {
  background: url(<%= asset_path 'wood.jpg'%>) 50% 0; color: #fff;
  .inside { max-width: 62.5em; margin: 0 auto;  padding: 0px 0 0;  box-shadow: 0 0 77px #000; background: rgba(0, 0, 0, .45); }
  .secondary {
    margin: 0 25px 0; padding: 120px 0 0; border-top: 1px solid #fff;
    .left { float: left; }
    .right { float: right; }
  }
}

