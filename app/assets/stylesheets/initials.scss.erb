body { line-height: 1; }
h1, h2, h3, h4, h5, h6 { clear: both; }
ul { list-style: none; }

blockquote {
  quotes: none;
  &:before, &:after {
    content: '';
    content: none;
  }
}

del { text-decoration: line-through; }

table {border-collapse: collapse; border-spacing: 0; }

img { display: block; }

input[type="submit"] {
  &::-moz-focus-inner, &::-webkit-focus-inner {
    border: 0;
  }
}
